{
  "name": "electron-winstaller",
  "version": "0.0.0-development",
  "description": "Module to generate Windows installers for Electron apps",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
